{
  "projectType": "application",
  "root": "apps/neekware",
  "sourceRoot": "apps/neekware/src",
  "prefix": "neekware",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "allowedCommonJsDependencies": ["zen-observable", "graphql-tag"],
        "outputPath": "dist/apps/neekware",
        "index": "apps/neekware/src/index.html",
        "main": "apps/neekware/src/main.ts",
        "polyfills": "apps/neekware/src/polyfills.ts",
        "tsConfig": "apps/neekware/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/neekware/src/favicon.ico",
          "apps/neekware/src/assets",
          {
            "glob": "**/*",
            "input": "libs/agx-assets/src/lib/styles/css",
            "output": "assets/css"
          },
          {
            "glob": "**/*",
            "input": "libs/agx-assets/src/lib/images",
            "output": "assets/images"
          },
          {
            "glob": "**/*(*.js|*.css)",
            "input": "tools/extensions",
            "output": "assets/code"
          },
          {
            "glob": "**/*",
            "input": "libs/agx-assets/src/lib/i18n/client",
            "output": "assets/i18n"
          },
          {
            "glob": "**/*",
            "input": "libs/agx-assets/src/lib/audio",
            "output": "assets/audio"
          },
          {
            "glob": "**/*.svg",
            "input": "node_modules/@mdi/angular-material",
            "output": "assets/fonts"
          },
          {
            "glob": "**/*.md",
            "input": "libs/ngx-chat/src/lib/doc",
            "output": "assets/md"
          },
          {
            "glob": "**/*.md",
            "input": "apps/neekware/src/app/pages/features",
            "output": "assets/md"
          }
        ],
        "styles": [
          "apps/neekware/src/styles.scss",
          "node_modules/prismjs/themes/prism-okaidia.css",
          "node_modules/prismjs/plugins/line-highlight/prism-line-highlight.css"
        ],
        "scripts": [
          "node_modules/jquery/dist/jquery.min.js",
          "node_modules/marked/lib/marked.js",
          "node_modules/prismjs/prism.js",
          "node_modules/prismjs/components/prism-javascript.min.js",
          "node_modules/prismjs/components/prism-typescript.min.js",
          "node_modules/prismjs/components/prism-css.min.js",
          "node_modules/prismjs/plugins/line-numbers/prism-line-numbers.js"
        ]
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2mb",
              "maximumError": "3mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "10kb",
              "maximumError": "15kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/neekware/src/environments/environment.ts",
              "with": "apps/neekware/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "electron": {
          "fileReplacements": [
            {
              "replace": "apps/neekware/src/environments/environment.ts",
              "with": "apps/neekware/src/environments/environment.electron.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "neekware:build:production",
          "proxyConfig": "apps/neekware/proxy/proxy.prod.json"
        },
        "ci": {
          "browserTarget": "neekware:build:production",
          "proxyConfig": "apps/neekware/proxy/proxy.ci.json"
        },
        "electron": {
          "browserTarget": "neekware:build:production",
          "proxyConfig": "apps/neekware/proxy/proxy.electron.json"
        },
        "development": {
          "browserTarget": "neekware:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "browserTarget": "neekware:build:development",
        "proxyConfig": "apps/neekware/proxy/proxy.dev.json"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "neekware:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/neekware/src/**/*.ts", "apps/neekware/src/**/*.html"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/neekware"],
      "options": {
        "jestConfig": "apps/neekware/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["app-client:neekware"],
  "implicitDependencies": ["agx-assets"]
}
